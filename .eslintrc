{
  "extends": ["airbnb-base", "plugin:react/recommended", "prettier"],
  "plugins": ["prettier", "react"],
  "rules": {
    "no-unused-vars": "warn",
    "prefer-const": "error",
    "no-var": "error",
    "object-shorthand": ["error", "always"],
    "eol-last": ["error", "always"],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "no-console": "warn",
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ]
  }
}
